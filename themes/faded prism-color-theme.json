{
	"name": "Faded Prism",
	"colors": {
		// Foreground Color 1 #d8d8d8
		"foreground": "#d8d8d8",
		"editor.foreground": "#d8d8d8",
		// Alt Color 1 Highlight #ab616b42
		"list.activeSelectionBackground": "#ab616b42",

		// Alt Color 1 Light #ab616b
		"focusBorder": "#ab616b",
		"activityBarBadge.background": "#ab616b",
		// Alt Color 1 Dark #593237
		"button.background": "#593237",
		// Alt Color 1 Darker #2a171a
		"statusBar.debuggingBackground": "#2a171a",
		// Dark Color 1 #181818
		"statusBar.background": "#181818",
		"statusBar.noFolderBackground": "#181818",
		"activityBar.background": "#181818",
		"banner.background": "#181818",
		"panel.background": "#181818",
		"toolbar.activeBackground": "#181818",
		"dropdown.background": "#181818",
		"tab.inactiveBackground": "#181818",
		"titleBar.activeBackground": "#181818",
		"titleBar.inactiveBackground": "#181818",
		"sideBar.background": "#181818",
		"editorGroupHeader.tabsBackground": "#181818",
		
		// Editor Background Color #252525
		"editor.background": "#252525",
		// Editor Bracket Highlighting
		"editorBracketHighlight.foreground1": "#fffd78",
		"editorBracketHighlight.foreground2": "#6cffa4",
		"editorBracketHighlight.foreground3": "#628cff",
		"editorBracketHighlight.foreground4": "#e552ff",
		"editorBracketHighlight.foreground5": "#ff4b90",
		"editorBracketHighlight.foreground6": "#ff915a",
	},
	"tokenColors": [
		{
			"name": "Keywords",
			"scope": [
				"punctuation.definition",
				"storage.modifier",
				"keyword",
				"keyword.operator.wordlike",
				"storage.type.primitive",
				"storage.type.var",
				"storage.type.function",
				"storage.type.built-in"
			],
			"settings": {
				"foreground": "#ff8d74"
			}
		},
		{
			"name": "Language Constants",
			"scope": [
				"keyword.constant",
				"constant.language",
			],
			"settings": {
				"foreground": "#f7afaf"
			}
		},
		{
			"name": "Punctuation, Operators",
			"scope": [
				"keyword.operator",
				"punctuation"
			],
			"settings": {
				"foreground": "#8b8b8b"
			}
		},
		{
			"name": "Class, Type",
			"scope": [
				"entity.name.class",
				"entity.name.type",
				"entity.other.inherited-class",
				"support.class",
				"storage.type",
				"support.type"
			],
			"settings": {
				"foreground": "#7bbdff" // "#a0f5ff"
			}
		},
		{
			"name": "Enum Member",
			"scope": [
				"variable.other.enummember"
			],
			"settings": {
				"foreground": "#b8e3ff"
			}
		},
		{
			"name": "Functions, Methods",
			"scope": [
				"entity.name.function",
				"entity.name.method",
				"support.function",
				"meta.function-call"
			],
			"settings": {
				"foreground": "#abff8d" // "#97ffc1"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"variable.parameter",
				"constant",
				"meta.function-call.arguments",
			],
			"settings": {
				"foreground": "#d8d8d8"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"variable.other.readonly",
				"constant"
			],
			"settings": {
				"foreground": "#b8e3ff"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"punctuation.definition.string",
				"storage.type.string"
			],
			"settings": {
				"foreground": "#fffa9f"
			}
		},
		{
			"name": "Numbers",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#f5c8ff"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#656565"
			}
		},
	]
}
