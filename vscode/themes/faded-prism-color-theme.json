{
	"name": "Faded Prism",
	"colors": {
		// white
		"foreground": "#d8d8d8",
		"editor.foreground": "#d8d8d8",
		// off_white
		"focusBorder": "#909090",
		// gray
		"button.background": "#4d4d4d",
		"list.activeSelectionBackground": "#4d4d4d",
		"editor.selectionBackground": "#4d4d4d",
		// dark_gray
		"editor.background": "#252525",
		// black
		"statusBar.background": "#181818",
		"statusBar.noFolderBackground": "#181818",
		"activityBar.background": "#181818",
		"banner.background": "#181818",
		"panel.background": "#181818",
		"toolbar.activeBackground": "#181818",
		"dropdown.background": "#181818",
		"tab.inactiveBackground": "#181818",
		"titleBar.activeBackground": "#181818",
		"titleBar.inactiveBackground": "#181818",
		"sideBar.background": "#181818",
		"editorGroupHeader.tabsBackground": "#181818",
		"activityBarBadge.foreground": "#181818",
		"statusBar.debuggingForeground": "#181818",
		// diff_red
		"editorGutter.deletedBackground": "#ff3e3e",
		// diff_yellow
		"editorGutter.modifiedBackground": "#ffea8a",
		// diff_green
		"editorGutter.addedBackground": "#6eff69",
		// error_red
		"editorError.foreground": "#ff3e3e",
		"errorForeground": "#ff3e3e",
		// warning_yellow
		"editorWarning.foreground": "#ffd932",
		// suggestion_blue
		"editorInfo.foreground": "#4fd6ff",
		// darkest_red
		"statusBar.debuggingBackground": "#ff8d74",
		// blue
		"activityBarBadge.background": "#7bbdff",
		// vibrant_yellow
		"editorBracketHighlight.foreground1": "#fffd78",
		// vibrant_green
		"editorBracketHighlight.foreground2": "#6cffa4",
		// vibrant_blue
		"editorBracketHighlight.foreground3": "#628cff",
		// vibrant_purple
		"editorBracketHighlight.foreground4": "#e552ff",
		// vibrant_red
		"editorBracketHighlight.foreground5": "#ff4b90",
		// vibrant_orange
		"editorBracketHighlight.foreground6": "#ff915a",
	},
	"tokenColors": [
		{
			"name": "Keywords",
			"scope": [
				"punctuation.definition",
				"storage.modifier",
				"keyword",
				"keyword.operator.wordlike",
				"storage.type"
			],
			"settings": {
				"foreground": "#ff8d74"
			}
		},
		{
			"name": "Language Constants",
			"scope": [
				"keyword.constant",
				"constant.language",
			],
			"settings": {
				"foreground": "#f7afaf"
			}
		},
		{
			"name": "Punctuation, Operators",
			"scope": [
				"keyword.operator",
				"punctuation"
			],
			"settings": {
				"foreground": "#909090"
			}
		},
		{
			"name": "Class, Type",
			"scope": [
				"entity.name.class",
				"entity.name.type",
				"entity.other.inherited-class",
				"support.class",
				"support.type"
			],
			"settings": {
				"foreground": "#7bbdff" // "#a0f5ff"
			}
		},
		{
			"name": "Enum Member",
			"scope": [
				"variable.other.enummember"
			],
			"settings": {
				"foreground": "#b8e3ff"
			}
		},
		{
			"name": "Functions, Methods",
			"scope": [
				"entity.name.function",
				"entity.name.method",
				"support.function",
				"meta.function-call"
			],
			"settings": {
				"foreground": "#abff8d" // "#97ffc1"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"variable.parameter",
				"constant",
				"meta.function-call.arguments",
			],
			"settings": {
				"foreground": "#d8d8d8"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"variable.other.readonly",
				"constant"
			],
			"settings": {
				"foreground": "#b8e3ff"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"punctuation.definition.string",
				"storage.type.string"
			],
			"settings": {
				"foreground": "#fffa9f"
			}
		},
		{
			"name": "Numbers",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#f5c8ff"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#6c6c6c"
			}
		},
	]
}
